{
  "manifest_version": 3,
  "name": "Quotes, Facts & Jokes Extension",
  "version": "1.1.4",
  "description": "Fetch and display quotes, facts, and jokes.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ],
  "icons": {
    "16": "icon96.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
